<org.eclipse.epf.uma:TaskDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:org.eclipse.epf.uma="http://www.eclipse.org/epf/uma/1.0.6/uma.ecore" xmlns:epf="http://www.eclipse.org/epf" epf:version="1.5.1" xmi:id="-9BI94_TvtnP7U2qSkWOYpg" name=",_ONIKQP9jEeGYqaC-voeltA" guid="-9BI94_TvtnP7U2qSkWOYpg" changeDate="2012-09-15T15:53:25.234-0300">
  <sections xmi:id="_c9tpsP9jEeGYqaC-voeltA" name="Conectar o smartphone ao computador." guid="_c9tpsP9jEeGYqaC-voeltA">
    <sectionDescription>&lt;p>&#xD;
    Conecte o smartphone ao computador que foi salvo o &lt;strong>APK&lt;/strong> através de um cabo &lt;strong>USB&lt;/strong>.&#xD;
&lt;/p>&#xD;
&lt;br />&#xD;
&lt;br /></sectionDescription>
  </sections>
  <sections xmi:id="_xQpXYP9jEeGYqaC-voeltA" name="Habilitar o Smartphone para se conectar ao computador." guid="_xQpXYP9jEeGYqaC-voeltA">
    <sectionDescription>&lt;p>&#xD;
    Habilite o Smartphone para se conectar ao computador. Abra a área de notificação e escolha a opção &lt;strong>“USB&#xD;
    conectado”.&lt;/strong>&#xD;
&lt;/p>&#xD;
&lt;p>&#xD;
    Observe a imagem abaixo:&#xD;
&lt;/p>&#xD;
&lt;p>&#xD;
    &lt;img height=&quot;309&quot; alt=&quot;&quot; src=&quot;resources/usb_conectado.jpg&quot; width=&quot;186&quot; />&#xD;
&lt;/p></sectionDescription>
  </sections>
  <sections xmi:id="_M-L1cP9kEeGYqaC-voeltA" name="Pressionar o botão “Ligar armazenamento USB”." guid="_M-L1cP9kEeGYqaC-voeltA">
    <sectionDescription>&lt;p>&#xD;
    Pressionar o botão “Ligar armazenamento USB” como mostra&amp;nbsp;a imagem abaixo:&lt;br />&#xD;
    &lt;br />&#xD;
    &lt;img height=&quot;308&quot; alt=&quot;&quot; src=&quot;resources/ligar_usb.jpg&quot; width=&quot;186&quot; />&#xD;
&lt;/p></sectionDescription>
  </sections>
  <sections xmi:id="_n3bIwP9kEeGYqaC-voeltA" name="Copiar o APK da Ferramenta Automatic Task Description." guid="_n3bIwP9kEeGYqaC-voeltA">
    <sectionDescription>&lt;p>&#xD;
    Copiar o APK da Ferramenta &lt;strong>Automatic Task Description&lt;/strong> que fará o mapeamento da aplicação a ser&#xD;
    avaliada.&#xD;
&lt;/p>&#xD;
&lt;br />&#xD;
&lt;br /></sectionDescription>
  </sections>
  <sections xmi:id="_9WDNgP9kEeGYqaC-voeltA" name="Instalar a Ferramenta Automatic Task Description." guid="_9WDNgP9kEeGYqaC-voeltA">
    <sectionDescription>&lt;p>&#xD;
    Instalar a Ferramenta &lt;strong>Automatic Task Description&lt;/strong>.&#xD;
&lt;/p>&#xD;
&lt;p>&#xD;
    &lt;img height=&quot;435&quot; alt=&quot;&quot; src=&quot;resources/install_atd.JPG&quot; width=&quot;600&quot; />&lt;br />&#xD;
    &lt;br />&#xD;
    &lt;br />&#xD;
&lt;/p></sectionDescription>
  </sections>
  <sections xmi:id="_9immUP9lEeGYqaC-voeltA" name="Copiar o APK da Aplicação." guid="_9immUP9lEeGYqaC-voeltA">
    <sectionDescription>&lt;p>&#xD;
    Copiar o APK da Aplicação que será avaliada.&#xD;
&lt;/p></sectionDescription>
  </sections>
  <sections xmi:id="_C4SRkP9mEeGYqaC-voeltA" name="Copiar o APK da Aplicação." guid="_C4SRkP9mEeGYqaC-voeltA">
    <sectionDescription>Copiar o APK da Aplicação que será avaliada.</sectionDescription>
  </sections>
  <purpose>Possibilitar que as tarefas que serão avaliadas sejam mapeadas.</purpose>
</org.eclipse.epf.uma:TaskDescription>
